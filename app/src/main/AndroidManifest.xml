<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gurmessa.firstapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:label="Login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SignUpUserActivity"
            android:label="Signup User" />
        <activity
            android:name=".SignUpDoctorActivity"
            android:label="Signup Doctor" />
        <activity
            android:name=".CheckSymptomsActivity"
            android:label="Check symptoms"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AnalyzeDataActivity"
            android:label="Analyze Data"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TestNavigationActivity"
            android:label="@string/title_activity_test_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SymptomsResultActivity" />
        <activity android:name=".PreventionActivity" />
        <activity android:name=".RiskFactorActivity" />
        <activity android:name=".AnalysisResultActivity"></activity>
    </application>

</manifest>